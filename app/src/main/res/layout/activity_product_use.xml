<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.AttendanceActivity"
    tools:ignore="HardcodedText, ContentDescription, RtlHardcoded,RtlSymmetry, SmallSp">

    <ImageView
        android:id="@+id/imageView64"
        android:layout_width="70dp"
        android:layout_height="53.3dp"
        android:background="@drawable/left_arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView330"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="화장품 직접 등록"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView64" />

    <View
        android:background="#F2F5F5"
        android:id="@+id/view97"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView64" />


    <TextView
        android:id="@+id/textview22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="화장품 분류"
        android:textColor="@color/black"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView64"/>

    <TextView
        android:id="@+id/textview221"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="화장품 분류"
        android:textColor="@color/black"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:textSize="16sp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE"/>

    <androidx.appcompat.widget.AppCompatSpinner
        android:id="@+id/spinnerPUSE3"
        android:layout_width="match_parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_height="40dp"
        android:visibility="gone"
        android:spinnerMode="dropdown"
        android:background="@drawable/button_product_use"
        app:layout_constraintTop_toBottomOf="@id/textview221"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatSpinner
        android:id="@+id/spinnerPUSE"
        android:layout_width="match_parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_height="40dp"
        android:spinnerMode="dropdown"
        android:background="@drawable/button_product_use"
        app:layout_constraintTop_toBottomOf="@id/textview22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/spinner_drop"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintTop_toTopOf="@id/spinnerPUSE"/>
    <ImageView
        android:id="@+id/dropimg"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:visibility="gone"
        android:src="@drawable/spinner_drop"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintTop_toTopOf="@id/spinnerPUSE3"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCheck"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="50dp"
        android:background="@drawable/button_product_use"
        android:backgroundTint="#F2F5F5"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"
        app:layout_constraintStart_toStartOf="parent"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCheck1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="50dp"
        android:visibility="invisible"
        android:background="@drawable/check_on_2"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/textview23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용안함"
        android:visibility="visible"
        android:textColor="@color/black"
        android:layout_marginTop="25dp"
        android:layout_marginStart="10dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/btnCheck"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"/>
    <TextView
        android:id="@+id/textview231"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용안함"
        android:visibility="invisible"
        android:textColor="@color/black"
        android:layout_marginTop="25dp"
        android:layout_marginStart="10dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/btnCheck1"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"/>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCheck2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="50dp"
        android:background="@drawable/button_product_use"
        android:backgroundTint="#F2F5F5"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"
        app:layout_constraintStart_toEndOf="@id/textview23"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCheck22"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="50dp"
        android:visibility="invisible"
        android:background="@drawable/check_on_2"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"
        app:layout_constraintStart_toEndOf="@id/textview23"/>

    <TextView
        android:id="@+id/textview29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용함"
        android:visibility="visible"
        android:textColor="@color/black"
        android:layout_marginTop="25dp"
        android:layout_marginStart="10dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/btnCheck2"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"/>
    <TextView
        android:id="@+id/textview291"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용함"
        android:visibility="invisible"
        android:textColor="@color/black"
        android:layout_marginTop="25dp"
        android:layout_marginStart="10dp"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/btnCheck22"
        app:layout_constraintTop_toBottomOf="@id/spinnerPUSE3"/>
    <TextView
        android:id="@+id/textview24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용 브랜드"
        android:textColor="@color/black"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textview23"/>

    <EditText
        android:id="@+id/edtProductUse"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/button_product_use"
        android:layout_marginStart="20dp"
        android:hint="사용 브랜드를 입력해 주세요"
        android:paddingLeft="10dp"
        android:textSize="14sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@id/textview24"/>

    <TextView
        android:id="@+id/textview25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="제품명"
        android:textColor="@color/black"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/edtProductUse"/>

    <EditText
        android:id="@+id/edtProductUse2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/button_product_use"
        android:layout_marginStart="20dp"
        android:hint="제품명를 입력해 주세요"
        android:paddingLeft="10dp"
        android:textSize="14sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@id/textview25"/>

    <TextView
        android:id="@+id/textview26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용기간"
        android:textColor="@color/black"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/edtProductUse2"/>

    <androidx.appcompat.widget.AppCompatSpinner
        android:id="@+id/spinnerPUSE2"
        android:layout_width="match_parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_height="40dp"
        android:background="@drawable/button_product_use"
        app:layout_constraintTop_toBottomOf="@id/textview26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/confirm_text"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@color/main_theme"
        android:fontFamily="@font/dream5"
        android:gravity="center"
        android:backgroundTint="@color/main_theme2"
        android:text="화장품 추가"
        android:textColor="#ffffff"
        android:textSize="16sp"
        tools:ignore="HardcodedText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/spinner_drop"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintTop_toTopOf="@id/spinnerPUSE2"/>







</androidx.constraintlayout.widget.ConstraintLayout>